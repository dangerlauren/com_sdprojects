<?xml version="1.0" encoding="utf-8"?>
<form enctype="multipart/form-data">
	<fieldset>
		<field
				name="id"
				type="hidden"
				/>
				
		<field
				name="year"
				type="text"
				label="Year"
				description="Year"
				size="100"
				class="inputbox"
				default=""
				/>
		<field
				name="semester"
				type="text"
				label="Semester"
				description="Semester"
				size="100"
				class="inputbox"
				default=""
				/>			
		<field
				name="title"
				type="text"
				label="Title"
				description="Title"
				size="100"
				class="inputbox"
				default=""
				/>
		<field
				name="students"
				type="text"
				label="Students"
				description="Students"
				size="200"
				class="inputbox"
				default=""
				/>	
		<field
				name="reqs"
				type="editor"
				label="Requirements"
				description="Requirements"
				width="1000"
				class="inputbox"
				default=""
				/>	
		<field
				name="problem"
				type="text"
				label="Problem"
				description="Problem"
				size="1000"
				class="inputbox"
				default=""
				/>	
		<field
				name="solution"
				type="text"
				label="Solution"
